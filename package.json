{
  "name": "exobase-api",
  "version": "0.0.1",
  "scripts": {
    "dev": "source ./env.sh && ts-node ./src/dev.ts",
    "build": "tsc",
    "token:builder": "source ./env.sh && ts-node ./src/local/createToken.ts --aud exo.builder --sub exo.builder --scopes 'deployment::read::elevated;deployment::update;instance::update::attributes;services::read'",
    "token:webhook": "source ./env.sh && ts-node ./src/local/createToken.ts --aud exo.webhook --sub exo.webhook --scopes 'services::read;services::deploy'",
    "make-types": "tsc --emitDeclarationOnly --declaration --outDir ./client/src"
  },
  "jest": {
    "preset": "ts-jest",
    "coverageThreshold": {
      "global": {
        "branches": 11,
        "functions": 36,
        "lines": 49,
        "statements": 48
      }
    }
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.12",
    "@types/oauth": "^0.9.1",
    "@types/uuid": "^8.3.3",
    "@vercel/node": "^1.12.1",
    "chai": "^4.3.4",
    "express": "^4.17.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@exobase/api": "^0.0.4",
    "@exobase/auth": "^0.0.11",
    "@exobase/core": "^0.0.4",
    "@exobase/hooks": "^0.0.7",
    "@exobase/local": "^0.0.17",
    "@exobase/vercel": "^0.0.9",
    "@magic-sdk/admin": "^1.3.2",
    "@octokit/auth-app": "^3.6.1",
    "@octokit/request": "^5.6.2",
    "@octokit/webhooks-types": "^4.17.1",
    "date-fns": "^2.26.0",
    "exo-use-github-webhook": "^0.0.2",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.2.0",
    "oauth": "^0.9.15",
    "radash": "^1.3.1",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  }
}
